<h1>My Favorite Recipes</h1>
<div class="row">
  <div ng-repeat="recipe in clients.myFavRecipes" class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="{{recipe.image}}">
      <div class="caption">
        <h3>{{recipe.title}}</h3>
        <p><a href="#" ng-click="clients.removeFave(recipe.id)" class="btn btn-primary" role="button">Remove From Favorites</a> <a href="{{recipe.sourceUrl}}" class="btn btn-default" role="button">Checkout Recipe</a></p>
      </div>
    </div>
   </div><!--end of div ng-repeat myFaveRecipes-->
</div><!--end div row-->